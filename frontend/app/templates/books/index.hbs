<h2>List</h2>
<div class="row">
{{#each model as |book|}}
     <div class="col-md-4">
 <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">{{book.title}}</h3>
    </div>
    <div class="panel-body">
               <p>Author: {{book.author.name}}</p>
        </div>
  </div>
</div>
{{/each}}</div>


